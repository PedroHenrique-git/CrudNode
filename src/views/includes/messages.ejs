<% if (errors.length > 0) { %>
    <div class="erros-container">
        <% errors.forEach(erro => { %>
            <%= erro %><br>
        <% }); %> 
    </div>
<% } %>

<% if (success.length > 0) { %>
    <div class="success-container">
        <% success.forEach(success => { %>
            <%= success %><br>
        <% }); %> 
    </div>
<% } %>